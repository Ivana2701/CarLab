@model CarLab.Models.CommonEntities.BasicDataModel

@{
    ViewData["Title"] = "User Profile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-5 col-md-5">
        <!-- Breadcubs Area Start Here -->
        <partial name="~/Views/Shared/_Breadcrumbs.cshtml" />
        <!-- Breadcubs Area End Here -->
    </div>



</div>


<div>

    <div class="col-12-xxxl col-xl-12 col-md-12">
        <div class="card account-settings-box">
            <div class="card-body">
                <div class="heading-layout1 mg-b-20">
                    <div class="item-title">
                        <h3>User Details</h3>
                    </div>
                    <div class="dropdown">
                       
                    </div>
                </div>
                <div class="user-details-box">
                    <div class="item-img">
                        <img src="~/themeContent/admin/img/figure/user.jpg" alt="user">
                    </div>
                    <div class="item-content">
                        <div class="info-table table-responsive">
                            <table class="table text-nowrap">
                                <tbody>
                                    <tr>
                                        <td>Name:</td>
                                        <td class="font-medium text-dark-medium">@Model.userObj.FullName</td>
                                    </tr>
                                    <tr>
                                        <td>Email Address:</td>
                                        <td class="font-medium text-dark-medium">@Model.userObj.EmailAddress</td>
                                    </tr>
                                    <tr>
                                        <td>Phone:</td>
                                        <td class="font-medium text-dark-medium">@Model.userObj.Phone</td>
                                    </tr>
                                    <tr>
                                        <td>City:</td>
                                        <td class="font-medium text-dark-medium">@Model.userObj.City</td>
                                    </tr>
                                   
                                  
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
